#!/bin/sh
alias gs="git status"
alias gc="git commit -am"
alias gd="git diff"
alias gitwhat="git log --reverse --pretty=format:'%C(yellow)%h%Creset %C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=short|grep"
alias gitmap="git log --graph --pretty=format:'<- %C(yellow)%h%Creset %C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=short"
alias gco="git checkout"
alias guys="gbs victor/master denis/master oleksii/master alex/master mark/master sergii/master"